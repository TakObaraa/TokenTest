    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.2.0",
    "@mantine/core": "^7.4.0",
    "@mantine/dates": "^7.4.1",
    "@mantine/hooks": "^7.4.0",
    "@mantine/notifications": "^7.4.1",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.6.5",
    "dayjs": "^1.11.9",
    "framer-motion": "^7.10.3",
    "history": "^5.3.0",
