{
  "name": "rl-app-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "postinstall": "lefthook install",
    "preview": "vite preview",
    "start": "vite --mode development",
    "start:storybook": "storybook dev -p 6006",
    "build": "tsc && vite build",
    "build:storybook": "storybook build",
    "gen:icons": "npx @svgr/cli --config-file .svgr/svgr.config.cjs -- .svgr/icons && npm run lint:fix:unsafe src/components/app/icons",
    "test:vitest": "vitest",
    "test:storybook": "test-storybook",
    "lint": "biome check",
    "lint:fix": "biome check --apply",
    "lint:fix:unsafe": "biome check --apply-unsafe",
    "fix:prettier": "prettier --write .",
    "fix:stylelint": "stylelint --fix .",
    "fix:eslint": "eslint --fix .",
    "transform": "token-transformer tokens/figma-tokens.json tokens/style-dictionary-tokens.json"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@dnd-kit/utilities": "^3.2.1",
